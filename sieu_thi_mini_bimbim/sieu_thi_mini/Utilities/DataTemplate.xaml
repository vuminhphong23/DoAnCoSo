    <ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:sieu_thi_mini.ViewModel"
                    xmlns:view="clr-namespace:sieu_thi_mini.View"    >
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:EmployeeVM}">
        <view:Employees/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <view:Order/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Products/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StatisticalVM}">
        <view:Statistical/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Setting/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ImportProductVM}">
        <view:ImportProducts/>
    </DataTemplate>



</ResourceDictionary>